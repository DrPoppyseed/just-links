{
  "hosting": {
    "public": "./web/build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/target/**",
      "/app-server"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "rewrites": [
    {
      "source": "**",
      "destination": "./web/app.html"
    }
  ]
}
