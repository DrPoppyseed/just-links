{
  "name": "web",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "prettier --plugin-search-dir src/ --check src/ && eslint src/",
    "format": "prettier --plugin-search-dir src/ --write ."
  },
  "dependencies": {
    "axios": "^1.5.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.23.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.28",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^3.59.2",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "vite": "^4.4.9"
  },
  "type": "module",
  "prettier": {
    "printWidth": 80,
    "plugins": [
      "prettier-plugin-svelte"
    ],
    "pluginSearchDirs": [
      "."
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "plugins": [
      "svelte3",
      "@typescript-eslint"
    ],
    "ignorePatterns": [
      "*.cjs"
    ],
    "overrides": [
      {
        "files": [
          "*.svelte"
        ],
        "processor": "svelte3/svelte3"
      }
    ],
    "settings": {
      "svelte3/typescript": "() => require('typescript')"
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2020
    },
    "env": {
      "browser": true,
      "node": true
    }
  }
}
